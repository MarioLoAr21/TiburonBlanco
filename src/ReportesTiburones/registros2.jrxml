	<queryString>
		<![CDATA[SELECT
     registro.`idPaquete` AS registro_idPaquete,
     registro.`idHorario` AS registro_idHorario,
     registro.`Categoria` AS registro_Categoria,
     registro.`Status_Pago` AS registro_Status_Pago,
     alumnos.`idAlumnos` AS alumnos_idAlumnos,
     alumnos.`nombre` AS alumnos_nombre,
     alumnos.`apellidoP` AS alumnos_apellidoP,
     alumnos.`apellidoM` AS alumnos_apellidoM,
     pagos.`folio` AS pagos_folio
FROM
     `alumnos` alumnos INNER JOIN `registro` registro ON alumnos.`idAlumnos` = registro.`idAlumno`
     INNER JOIN `pagos` pagos ON alumnos.`idAlumnos` = pagos.`idAlumno`
WHERE
     registro.`Status_Pago` = 'Pagado']]>
	</queryString>
